generator client {
  provider = "prisma-client"
  output      = "../src/generated/prisma"
  moduleFormat    = "cjs"
}

datasource db {
  provider = "postgresql"
}

model usuarios{
  id String @id @default(uuid())
  nome String
  email String @unique
  senha String
  role String @default("USER")
}
